{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "EnterpriseAgent": {
    "ApiBaseUrl": "https://api.enterprise.local",
    "NodeIdentification": {
      "UseComputerName": true,
      "UseUserName": true
    },
    "Services": {
      "Email": {
        "Enabled": true,
        "SyncIntervalMinutes": 5
      },
      "Backup": {
        "Enabled": true,
        "CheckIntervalMinutes": 15
      },
      "RemoteSupport": {
        "Enabled": true
      },
      "WhatsApp": {
        "Enabled": false
      },
      "VoIP": {
        "Enabled": false
      },
      "AI": {
        "Enabled": false
      }
    }
  }
}